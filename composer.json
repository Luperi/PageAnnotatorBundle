  "keywords": ["html", "annotator", "annotate", "annotatorjs"],
  "homepage": "https://github.com/Luperi/PageAnnotatorBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Luca Perico",
      "email": "pericoluca1993@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.3.3",
    "symfony/framework-bundle": "2.*",
    "symfony/finder": "2.*",
    "symfony/yaml": "2.*",
    "symfony/form": "2.*",
    "symfony/browser-kit": "2.*",
    "friendsofsymfony/jsrouting-bundle": "2.*"
  },
  "minimum-stability": "dev",
  "target-dir": "Luperi/PageAnnotatorBundle",
  "autoload": {
    "psr-0": { "Luperi\\PageAnnotatorBundle": "" }
